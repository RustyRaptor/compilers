   26 Statements: %empty
   27           | Statement Statements

   28 Statement: Block
   29          | Assign ';'
   30          | MethodCall ';'
   31          | T_IF '(' Expr ')' Block T_ELSE Block
   32          | T_IF '(' Expr ')' Block
   33          | T_WHILE '(' Expr ')' Block
   34          | T_RETURN ';'
   35          | T_RETURN '(' ')' ';'
   36          | T_RETURN '(' Expr ')' ';'
   37          | T_BREAK ';'
   38          | T_CONTINUE ';'

   39 Assign: Lvalue T_ASSIGN Expr

   40 Lvalue: T_ID
   41       | T_ID '[' Expr ']'

   42 MethodCall: T_ID '(' MethodArgs ')'

   43 MethodArg: Expr

   44 MethodArgs: %empty
   45           | FullMethodArgs

   46 FullMethodArgs: MethodArg
   47               | MethodArg ',' MethodArgs

   48 Expr: Simpleexpression

   49 Simpleexpression: Additiveexpression
   50                 | Simpleexpression Relop Additiveexpression

   51 Relop: T_LEQ
   52      | '<'
   53      | T_GT
   54      | T_GEQ
   55      | T_EQ
   56      | T_NEQ

   57 Additiveexpression: Term
   58                   | Additiveexpression Addop Term

   59 Addop: '+'
   60      | '-'

   61 Term: Factor
   62     | Term Multop Factor

   63 Multop: '*'
   64       | '/'
   65       | '%'
   66       | T_AND
   67       | T_OR
   68       | T_LEFTSHIFT
   69       | T_RIGHTSHIFT

   70 Factor: T_ID
   71       | MethodCall
   72       | T_ID '[' Expr ']'
   73       | Constant
   74       | '(' Expr ')'
   75       | '!' Factor
   76       | '-' Factor

   77 ExternType: T_STRINGTYPE
   78           | Type

   79 Type: T_INTTYPE
   80     | T_BOOLTYPE

   81 MethodType: T_VOID
   82           | Type

   83 BoolConstant: T_TRUE
   84             | T_FALSE

   85 ArrayType: '[' T_INTCONSTANT ']' Type

   86 Constant: T_INTCONSTANT
   87         | T_STRINGCONSTANT
   88         | T_CHARCONSTANT
   89         | BoolConstant


